(i,j,k,count)=(0,0,0,0)\n","  while k<m+n:\n","    if i==m:\n","      C.extend(B[j:])\n","      k+=n-j\n","    elif j==n:\n","      C.extend(A[i:])\n","      k+=m-i\n","    elif A[i]<B[j]:\n","      C.append(A[i])\n","      (i,k)=(i+1,k+1)\n","    else:\n","      C.append(B[j])\n","      (j,k,count)=(j+1,k+1,count+(m-i))\n","  return(C,count)\n","\n","def inversions(A):\n","  n = len(A)\n","  if n<=1:\n","    return(A,0)\n","  (L,x) = inversions(A[:n//2])\n","  (R,y) = inversions(A[n//2:])\n","  (B,z) = merge_and_count(L,R)\n","  return(B, x+y+z)\n","\n","def countIntersection(x1, x2):\n","    z = list(zip(x1, x2))\n","    print(z)\n","    z.sort()\n","    print(z)\n","    A = [i[1] for i in z]\n","    print(A)\n","    L, ans = inversions(A)\n","    return ans\n","L1 = [int(i) for i in input().split(\" \")]\n","L2 = [int(i) for i in input().split(\" \")]\n","timeout = 2.0  # Timeout in sec"],"metadata":{"id":"L8yBtO3mCi02","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1690916988967,"user_tz":-330,"elapsed":24959,"user":{"displayName":"Gauranga Kumar Baishya","userId":"00723650175688021252"}},"outputId":"e004e636-3856-40ea-98e5-2d162c316db4"},"execution_count":null,"outputs":[{"name":"stdout","output_type":"stream","text":["8 22 10 4 17 14\n","15 18 3 12 10 23\n"]}]},{"cell_type":"code","source":["countIntersection(L1, L2)"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"vmDPVX3Ifpg3","executionInfo":{"status":"ok","timestamp":1690916992483,"user_tz":-330,"elapsed":548,"user":{"displayName":"Gauranga Kumar Baishya","userId":"00723650175688021252"}},"outputId":"1b205653-aed8-48e8-a76b-d682c81086ce"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["[(8, 15), (22, 18), (10, 3), (4, 12), (17, 10), (14, 23)]\n","[(4, 12), (8, 15), (10, 3), (14, 23), (17, 10), (22, 18)]\n","[12, 15, 3, 23, 10, 18]\n"]},{"output_type":"execute_result","data":{"text/plain":["6"]},"metadata":{},"ex